# .bash_profile
# ln -s path/to/this/file  ~/.bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

# User specific environment and startup programs

PATH=$HOME/.local/bin:$HOME/bin:$PATH

export PATH

export SHELL=$HOME/.local/bin/fish
exec fish

# Local Variables:
# mode: sh
# End:
